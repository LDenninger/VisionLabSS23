{
    "layers": [
        {
            "type": "ResNet",
            "size": 18,
            "remove_layer": 1,
            "weights": "DEFAULT"
        },
        {
            "type": "flatten"
        },
        {
            "type": "dropout",
            "prob": 0.3
        },
        {
            "type": "linear",
            "in_features": 512,
            "out_features": 2
        }
    ],
    "output_dim": 2,
    "activation": "relu",
    "batch_size": 32,
    "num_epochs": 20,
    "num_iterations": 62,
    "learning_rate": 3e-05,
    "random_seed": 22,
    
    "pre_processing": [
        {
            "type": "mixup",
            "alpha": 1.0,
            "prob": 0.5,
            "beta": 1.0,
            "num_classes": 2,
            "train": true,
            "eval": false
        },
        {
            "type": "random_rotation",
            "degrees": 5,
            "train": true,
            "eval": false
        },
        {
            "type": "random_horizontal_flip",
            "prob": 0.3,
            "train": true,
            "eval": false
        },
        {
            "type": "color_jitter",
            "brightness": 0.4,
            "contrast": 0.4,
            "saturation": 0.3,
            "hue": 0.0,
            "train": true,
            "eval": false
        },
        {
            "type": "gaussian_blur",
            "kernel_size": [
                5,
                5
            ],
            "sigma": [
                0.1,
                2.0
            ],
            "train": true,
            "eval": false
        },
        {
            "type": "normalize",
            "train": true,
            "eval": true
        }
    ],
    "evaluation": {
        "metrics": [
            "accuracy"
        ],
        "frequency": 1
    },
    "dataset": {
        "name": "oxfordpets",
        "train_size": 2000,
        "val_size": 0,
        "test_size": 3669,
        "train_shuffle": true,
        "eval_shuffle": false,
        "drop_last": true,
        "classes": [
            0,
            1
        ]
    },
    "scheduler": {
        "epoch_scheduler": {
            "type": "exponential",
            "gamma": 0.9
        },
        "iteration_scheduler": null
    },
    "optimizer": {
        "type": "Adam"
    }
}